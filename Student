marks=[]
n=int(input("Enter no of students:"))
for i in range(n):
    stu=int(input("Enter marks of student in FDS, If absent enter 0:"))
    marks.append(stu)
    
def average():
    avg=0
    sum1=0
    ab=0;
    for i in range(n):
        if marks[i]==0:
            ab+=1 
        else:
            sum1+=marks[i]
    avg=sum1/(n-ab)
    print("Average:",avg)
    
def maximum():
    maximum=marks[0]
    for i in marks:
        if i>maximum:
            maximum=i
    print("Maximum:",maximum)
    
def minimum():
    minimum=marks[0]
    for i in marks:
        if i<minimum:
            minimum=i 
    print("Minimum:",minimum)
    
def absent():
    ab=0
    for i in marks:
        if i==0:
            ab=ab+1
    print("Absent students:",ab)
    
    
def fre():
    count=marks[0]
    for i in marks:
        if i==count:
            count=i
    print("Maximum Frequency:",count)
    
def mainmenu():
    print("\n1.Average\n2.Maximum and Minimum\n3.Absent\n4.Frequency\n5.Exit")
    
while True:
    mainmenu()
    ch=int(input("Enter a choice:"))
    if ch==1:
        average()
    elif ch==2:
        maximum()
        minimum()
    elif ch==3:
        absent()
    elif ch==4:
        fre()
    elif ch==5:
        False
        break
    else:
        print("Enter a valid choice")
    
