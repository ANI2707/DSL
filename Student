marks=[]
n=int(input("Enter the no of students:"))
for i in range(n):
    stu=int(input("Enter the marks of students in FDS, if student is absent enter 0:"))
    marks.append(stu)
    
def avg():
    ab=0
    sum1=0
    for i in range(n):
        if i==0:
            ab+=1
        else:
            sum1+=marks[i]
    print("Average:",sum1/(n-ab))
    
def absent():
    ab=0
    for i in marks:
        if i==0:
            ab+=1 
    print("Absent:",ab)
    
def maximum():
    maximum=marks[0]
    for i in marks:
        if i>maximum:
            maximum=i 
    print("Maximum:",maximum)

def minimum():
    minimum=marks[0]
    for i in marks:
        if i==0:
            continue
        elif i<minimum:
            minimum=i 
    print("Minimum:",minimum)
    
def frequency():
    fre=max(set(marks),key=marks.count)
    print("Frequency:",fre)
    
def mainmenu():
    print("\n1.Average\n2.Maximum and Minimum\n3.Absent\n4.Frequency\n5.Exit")
    
while True:
    mainmenu()
    ch=int(input("Enter a choice:"))
    if ch==1:
        avg()
    elif ch==2:
        maximum()
        minimum()
    elif ch==3:
        absent()
    elif ch==4:
        frequency()
    elif ch==5:
        False
        break
    else:
        print("Enter a valid choice")


    
