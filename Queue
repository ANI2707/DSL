#include<bits/stdc++.h>
using namespace std;
class Queue{
    public:
    int r,f,num,arr[20];
    Queue(){
        r=-1;
        f=-1;
    }
    
    void insert(){
        cout<<"Enter the element to insert:";
        cin>>num;
        if(r==-1 && f==-1){
            r=r+1;
            f=f+1;
            arr[r]=num;
        }
        else{
            r=r+1;
            arr[r]=num;
        }
    }
    
    void delete1(){
        
        num=arr[f];
        if(r==f){
            r=-1;
            f=-1;
        
        }
        else{
            f=f+1;
            
        }
        cout<<"Deleted Element"<<num<<endl;
    }
    
    void display(){
        for(int i=f;i<=r;i++){
            cout<<arr[i]<<endl;
        }
    }

};
int main(){
    int ch;
    Queue q;
    do{
        cout<<"\n1.Insert\n2.Delete\n3.Display\n4.Exit";
        cout<<"\nEnter a choice:";
        cin>>ch;
        switch(ch){
            case 1:
            q.insert();
            break;
            case 2:
            q.delete1();
            break;
            case 3:
            q.display();
            break;
            
        }
    }
    while(ch!=4);
    return 0;
}
