#include<bits/stdc++.h>
using namespace std;
class node{
	public:
		string data;
		node* left;
		node* right;
};
class tree{
	public:
		node* root;
		tree(){
			root=NULL;
		}
		node* create(){
			string key;
			cout<<"Enter the book(if not enter NULL):";
			cin>>key;
			if(key=="NULL"){
			    return NULL;
			}
			node* root=new node();
			root->data=key;
			root->left=NULL;
			root->right=NULL;
		    cout<<"Enter the left part(if not enter NULL):"<<key<<endl;
			root->left=create();
			cout<<"Enter the right part(if not enter NULL):"<<key<<endl;
			root->right=create();
			
			return root;
		}
		void preorder(node* t){
			if(t==NULL)
			    return;
			cout<<t->data<<" ";
			preorder(t->left);
			preorder(t->right);
		}
		void inorder(node* t){
			if(t==NULL)
			    return;
			inorder(t->left);
			cout<<t->data<<" ";
			inorder(t->right);
			
		}
		void postorder(node* t){
			if(t==NULL)
			    return;
			postorder(t->left);
			postorder(t->right);
			cout<<t->data<<" ";
		}
};
int main(){
	tree t;
	int ch;
	while(1){
		cout<<"\nEnter a choice:";
		cin>>ch;
		switch(ch){
			case 1:
				t.root=t.create();
				break;
			case 2:
				t.preorder(t.root);
				break;
			case 3:
			    t.inorder(t.root);
			    break;
			case 4:
			    t.postorder(t.root);
			    break;
			case 5:
			    exit(1);
		}
	}
	return 0;
}
